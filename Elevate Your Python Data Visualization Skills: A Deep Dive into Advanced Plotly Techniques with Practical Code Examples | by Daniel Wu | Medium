np.pi, <span class="hljs-number">100</span>)<br>x = np.sin(t)<br>y = np.cos(t)<br><span class="hljs-comment"># Create an animated line plot</span><br>fig = go.Figure(data=go.Scatter(x=[<span class="hljs-number">0</span>], y=[<span class="hljs-number">0</span>], mode=<span class="hljs-string">'lines'</span>), layout=go.Layout(updatemenus=[<span class="hljs-built_in">dict</span>(<span class="hljs-built_in">type</span>=<span class="hljs-string">'buttons'</span>, showactive=<span class="hljs-literal">False</span>, buttons=[<span class="hljs-built_in">dict</span>(label=<span class="hljs-string">'Play'</span>, method=<span class="hljs-string">'animate'</span>, args=[<span class="hljs-literal">None</span>, <span class="hljs-built_in">dict</span>(frame=<span class="hljs-built_in">dict</span>(duration=<span class="hljs-number">100</span>, redraw=<span class="hljs-literal">True</span>), fromcurrent=<span class="hljs-literal">True</span>)])])]))<br><span class="hljs-comment"># Add frames for animation</span><br>frames = [go.Frame(data=go.Scatter(x=x[:i], y=y[:i], mode=<span class="hljs-string">'lines+markers'</span>)) <span class="hljs-keyword">for</span> i <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">2</span>, <span class="hljs-built_in">len</span>(t))]<br>fig.frames = frames<br><span class="hljs-comment"># Add title and labels</span><br>fig.update_layout(title=<span class="hljs-string">'Animated Line Plot'</span>, xaxis_title=<span class="hljs-string">'X-axis'</span>, yaxis_title=<span class="hljs-string">'Y-axis'</span>, updatemenus=[<span class="hljs-built_in">dict</span>(<span class="hljs-built_in">type</span>=<span class="hljs-string">'buttons'</span>, showactive=<span class="hljs-literal">False</span>, buttons=[<span class="hljs-built_in">dict</span>(label=<span class="hljs-string">'Play'</span>, method=<span class="hljs-string">'animate'</span>, args=[<span class="hljs-literal">None</span>, <span class="hljs-built_in">dict</span>(frame=<span class="hljs-built_in">dict</span>(duration=<span class="hljs-number">100</span>, redraw=<span class="hljs-literal">True</span>), fromcurrent=<span class="hljs-literal">True</span>)])])])<br><span class="hljs-comment"># Show the plot</span><br>fig.show()</span></pre><figure class="od oe of og oh lw lo lp paragraph-image"><div role="button" tabindex="0" class="lx ly ee lz bg ma"><div class="lo lp ox"><picture><source srcset="https://miro.medium.com/v2/resize:fit:640/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 640w, https://miro.medium.com/v2/resize:fit:720/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 720w, https://miro.medium.com/v2/resize:fit:750/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 750w, https://miro.medium.com/v2/resize:fit:786/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 786w, https://miro.medium.com/v2/resize:fit:828/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 828w, https://miro.medium.com/v2/resize:fit:1100/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 1100w, https://miro.medium.com/v2/resize:fit:1400/format:webp/1*DTpwBxh-PCZpGUJ8GECOLg.png 1400w" sizes="(min-resolution: 4dppx) and (max-width: 700px) 50vw, (-webkit-min-device-pixel-ratio: 4) and (max-width: 700px) 50vw, (min-resolution: 3dppx) and (max-width: 700px) 67vw, (-webkit-min-device-pixel-ratio: 3) and (max-width: 700px) 65vw, (min-resolution: 2.5dppx) and (max-width: 700px) 80vw, (-webkit-min-device-pixel-ratio: 2.5) and (max-width: 700px) 80vw, (min-resolution: 2dppx) and (max-width: 700px) 100vw, (-webkit-min-device-pixel-ratio: 2) and (max-width: 700px) 100vw, 700px" type="image/webp"><source data-testid="og" srcset="https://miro.medium.com/v2/resize:fit:640/1*DTpwBxh-PCZpGUJ8GECOLg.png 640w, https://miro.medium.com/v2/resize:fit:720/1*DTpwBxh-PCZpGUJ8GECOLg.png 720w, https://miro.medium.com/v2/resize:fit:750/1*DTpwBxh-PCZpGUJ8GECOLg.png 750w, https://miro.medium.com/v2/resize:fit:786/1*DTpwBxh-PCZpGUJ8GECOLg.png 786w, https://miro.medium.com/v2/resize:fit:828/1*DTpwBxh-PCZpGUJ8GEC